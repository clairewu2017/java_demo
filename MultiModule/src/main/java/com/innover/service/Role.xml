<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.innover.service.Role">
        <table name="Role" schema="dbo" catalog="moocprojectv2"/>
        <attributes>
            <id name="id">
                <column name="Id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="Name" length="50"/>
            </basic>
            <basic name="isProtected">
                <column name="IsProtected" precision="-1"/>
            </basic>
            <basic name="hideRole">
                <column name="HideRole" precision="-1"/>
            </basic>
            <one-to-many name="permissionsById" mapped-by="roleByRoleId"
                         target-entity="com.innover.service.Permission"/>
            <one-to-many name="userRolesById" mapped-by="roleByRoleId" target-entity="com.innover.service.UserRoles"/>
        </attributes>
    </entity>
</entity-mappings>
